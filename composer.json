{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
		{
            "type": "package",
            "package": {
                "name": "desandro/masonry",
                "version": "3.3.1",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/desandro/masonry/archive/v3.3.1.zip",
                    "type": "zip"
                }
            }
        },
		{
            "type": "package",
            "package": {
                "name": "desandro/imagesloaded",
                "version": "3.1.8",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/desandro/imagesloaded/archive/v3.1.8.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "enyo/dropzone",
                "version": "4.2.0",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/enyo/dropzone",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "desandro/imagesloaded": "3.1.8",
        "desandro/masonry": "3.3.1",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/addtoany": "^1.10",
        "drupal/admin_toolbar": "^1.25",
        "drupal/bootstrap": "^3.20",
        "drupal/console": "^1.0.2",
        "drupal/core": "^8.7.4",
        "drupal/devel": "^1.2",
        "drupal/dropzonejs": "^2.0",
        "drupal/entity_browser": "^2.0",
        "drupal/entity_embed": "^1.0",
        "drupal/file_browser": "^1.1",
        "drupal/material_admin": "^1.0",
        "drupal/metatag": "^1.7",
        "drupal/paragraphs": "^1.5",
        "drupal/paragraphs_browser": "^1.0",
        "drupal/pathauto": "^1.3",
        "drupal/simplelogin": "^3.3",
        "drupal/stage_file_proxy": "^1.0",
        "drupal/theone": "^1.1",
        "drupal/twig_tweak": "^2.1",
        "drush/drush": "^9.0.0",
        "enyo/dropzone": "4.2.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-core-require-dev": "^8.7.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "platform": {
          "php": "7.2.14"
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "dcl-start": [
            "./docker-compose-lamp/scripts/server-start.sh"
        ],
        "dcl-ssh": [
            "./docker-compose-lamp/scripts/server-ssh.sh"
        ]
    },
    "extra": {
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
